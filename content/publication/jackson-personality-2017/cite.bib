@article{jackson_personality_2017,
 abstract = {Numerous studies that link personality traits with health status, sometimes decades in the future, providing a strong claim for personality traits to causally influence physical health. While the prevailing consensus is that personality traits influence health through intermediary processes like health behaviors, less discussed is the possibility that health status influences personality. In this chapter, we describe the existing evidence that health is associated with changes in personality and discuss the potential mechanisms that relate the two.},
 author = {Jackson, Joshua J and Weston, Sara J and Schultz, Leah H},
 copyright = {Creative Commons Attribution-ShareAlike 4.0 International License (CC-BY-SA)},
 doi = {10.1016/b978-0-12-804674-6.00023-5},
 journal = {Part Five: Personality Development in Context},
 pages = {371--384},
 title = {Personality Development Across the Lifespan},
 year = {2017}
}
